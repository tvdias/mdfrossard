  <link rel="canonical" href="<%= canonical_url %>" />
  <link rel="icon" href="/images/favicon.ico" type="image/x-icon"/>  
  <link rel="alternate" type="application/atom+xml" title="Feed para <%= config.title %>" href="<%= config.url %>/atom.xml" />
  <meta name="author" content="<%= config.title %>" /> 
  <meta name="Publisher" content="<%= config.title %>" /> 
  <meta name="Googlebot" content="all" />

  <%- open_graph({
    fb_app_id: config.fb_app_id,
    google_plus: config.google_plus,
    twitter_id: config.twitter_id,
    image: page.featured_image
  }) %>

  <meta name="google-site-verification" content="RV8HX__uJPpEeGNG6yucFLEEK-DJ7WPZRH5siZpSaAA" />
  <meta name="google-site-verification" content="0B5-KOJcNwtZwe5yXQGcZhz55uC7PBLEXatlWo0HEE4" />
  
  <%- partial('fb-pixel') %>
  
  <% if (is_post() && config.generator_amp){ %>
    <link rel="amphtml" href="<%= canonical_url %>amp/index.html" />
  <% } %>
</head>